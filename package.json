  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.18",
    "@types/memory-fs": "^0.3.2",
    "@types/node": "^12.7.3",
    "@types/react": "^16.9.2",
    "@types/webpack": "^4.39.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^24.9.0",
    "memory-fs": "^0.4.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.2",
    "react": "^16.9.0",
    "rimraf": "^3.0.0",
    "ts-loader": "^8.1.0",
    "typescript": "^4.3.2",
    "webpack": "^4.39.3"
  }
}
